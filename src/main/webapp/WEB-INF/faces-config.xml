<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
     <application>
     	   <locale-config>
     	        <default-locale>es</default-locale>
     	        <supported-locale>en</supported-locale>
     	   </locale-config>
	   <resource-bundle>
		<base-name>com.ecodeup.lang</base-name>
		<var>translate</var>
	   </resource-bundle>
     </application>
	<managed-bean>
		<managed-bean-name>productoBean</managed-bean-name>
		<managed-bean-class>com.ecodeup.beans.ProductoBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>barrioBean</managed-bean-name>
		<managed-bean-class>com.ecodeup.beans.barrioBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>rolBean</managed-bean-name>
		<managed-bean-class>com.ecodeup.beans.rolBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>tipodocumentoBean</managed-bean-name>
		<managed-bean-class>com.ecodeup.beans.tipodocumentoBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>ciudadBean</managed-bean-name>
		<managed-bean-class>com.ecodeup.beans.ciudadBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>loteBean</managed-bean-name>
		<managed-bean-class>com.ecodeup.beans.loteBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

</faces-config>